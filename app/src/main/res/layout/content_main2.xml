<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/app_bar_main2">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_33sdp"
        android:layout_gravity="top"
        app:cardCornerRadius="20dp"
        android:layout_marginStart="@dimen/_18sdp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="@dimen/_64sdp"
        android:layout_marginEnd="@dimen/_18sdp"
        app:cardElevation="@dimen/_10sdp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/button2"
                android:layout_width="@dimen/_25sdp"
                android:layout_height="match_parent"
                android:layout_gravity="end|center_vertical"
                android:layout_marginStart="@dimen/_9sdp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="6dp"
                android:layout_weight="1"
                android:fontFamily="@font/lemon"

                android:text="تم"
                android:textColor="#ff000000"
                android:textSize="@dimen/_8ssp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="bottom|start"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="@dimen/_16sdp"
                android:fontFamily="@font/janna"
                android:layout_marginRight="8dp"
                android:textAlignment="center"
                android:elevation="@dimen/_10sdp"
                android:layout_weight="7"
                android:defaultFocusHighlightEnabled="true"
                android:textStyle="bold"
                android:hint=" ابحث هنا"

                android:textSize="@dimen/_11ssp"

              />




        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="@dimen/_18sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="@dimen/_15sdp"
        app:cardCornerRadius="10dp"
        app:cardElevation="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView3">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text="الاقسام"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:fontFamily="@font/lemon"
            android:textColor="#B3000000"
            android:textSize="@dimen/_11sdp"
            />
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_330sdp"
            android:columnCount="2"
            android:rowCount="3"
            android:layout_marginTop="@dimen/_42sdp"
            android:orientation="vertical"


            >

            <androidx.cardview.widget.CardView
                android:id="@+id/golds"
                android:layout_width="@dimen/_128sdp"
                android:layout_height="@dimen/_97sdp"
                android:layout_column="0"
                android:layout_row="0"
                android:layout_gravity="top|left"
                app:cardElevation="3dp"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="6dp"
                android:layout_marginEnd="6dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                app:cardCornerRadius="3dp"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                >
                <ImageView
                    android:id="@+id/thumbnail"
                    android:layout_width="match_parent"
                    android:scaleType="centerCrop"
                    android:layout_height="match_parent"
                    android:background="@color/photo_placeholder" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    >



                    <TextView
                        android:id="@+id/signed_in"
                        android:background="@drawable/back_style"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:elevation="3dp"
                        android:text="مجوهرات"
                        android:textAlignment="center"
                        android:textColor="#fff"
                        android:textSize="@dimen/_13ssp"
                        android:fontFamily="@font/janna"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/cars"
                android:layout_width="@dimen/_128sdp"
                android:layout_height="@dimen/_97sdp"
                android:layout_column="0"
                android:layout_row="1"
                android:layout_gravity="top|left"
                app:cardElevation="3dp"
                app:cardCornerRadius="3dp"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="6dp"
                android:layout_marginEnd="6dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                >
                <ImageView
                    android:layout_width="match_parent"
                    android:scaleType="centerCrop"
                    android:layout_height="match_parent"
                    android:background="@color/photo_placeholder" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    >



                    <TextView
                        android:id="@+id/car"
                        android:background="@drawable/back_style"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:elevation="3dp"
                        android:text="سيارات"
                        android:textAlignment="center"
                        android:textColor="#fff"
                        android:textSize="@dimen/_13ssp"
                        android:fontFamily="@font/janna"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_128sdp"
                android:layout_height="@dimen/_97sdp"
                android:id="@+id/others"
                android:layout_column="0"
                android:layout_row="2"
                android:layout_gravity="top|left"
                app:cardElevation="3dp"
                app:cardCornerRadius="3dp"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="6dp"
                android:layout_marginEnd="6dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                >
                <ImageView
                    android:layout_width="match_parent"
                    android:scaleType="centerCrop"
                    android:layout_height="match_parent"
                    android:background="@color/photo_placeholder" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    >



                    <TextView
                        android:id="@+id/other"
                        android:background="@drawable/back_style"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:elevation="3dp"
                        android:text="اشياء اخري"
                        android:textAlignment="center"
                        android:textColor="#fff"
                        android:textSize="@dimen/_13ssp"
                        android:fontFamily="@font/janna"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/vehicals"
                android:layout_width="@dimen/_128sdp"
                android:layout_height="@dimen/_97sdp"
                android:layout_column="1"
                android:layout_row="0"
                android:layout_gravity="top|right"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="6dp"
                android:layout_marginStart="6dp"
                app:cardElevation="3dp"
                app:cardCornerRadius="3dp"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                >
                <ImageView
                    android:layout_width="match_parent"
                    android:scaleType="centerCrop"
                    android:layout_height="match_parent"
                    android:background="@color/photo_placeholder" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    >



                    <TextView
                        android:id="@+id/vehical"
                        android:background="@drawable/back_style"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:elevation="3dp"
                        android:text="مركبات"
                        android:textAlignment="center"
                        android:textColor="#fff"
                        android:textSize="@dimen/_13ssp"
                        android:fontFamily="@font/janna"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/builds"
                android:layout_width="@dimen/_128sdp"
                android:layout_height="@dimen/_97sdp"
                android:layout_column="1"
                android:layout_row="1"
                android:layout_gravity="top|right"
                app:cardElevation="3dp"
                app:cardCornerRadius="3dp"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="6dp"
                android:layout_marginStart="6dp"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                >
                <ImageView
                    android:layout_width="match_parent"
                    android:scaleType="centerCrop"
                    android:layout_height="match_parent"
                    android:background="@color/photo_placeholder" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    >



                    <TextView
                        android:id="@+id/build"
                        android:background="@drawable/back_style"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:elevation="3dp"
                        android:text="عقارات"
                        android:textAlignment="center"
                        android:textColor="#fff"
                        android:textSize="@dimen/_13ssp"
                        android:fontFamily="@font/janna"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

        </GridLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>